{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"alwaysStrict": true,
		"lib": [
			"ES2020",
			"DOM"
		],
		"module": "ES2020",
		"moduleResolution": "Node",
		"noImplicitAny": true,
		"noUnusedParameters": true,
		"pretty": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES2020",
	},
	"files": [
		"src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}
